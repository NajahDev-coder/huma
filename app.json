{
  "name": "HüMA",
  "displayName": "HüMA",
  "expo": {
    "name": "HüMA",
    "slug": "HuMA",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.3",
    "runtimeVersion": "1.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "css",
        "ttf"
      ],
      "transformer": "node_modules/react-native-css-transformer/index.js"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "http://localhost:3000/api/manifest"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devnajah.HuMA"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5e3e1641-a3b7-4c08-a955-23c9b9e1ba09"
      }
    },
    "android": {
      "package": "com.devnajah.HuMA",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAVWheD_CJmbOlCCKBTRKRRkeFJy_Mxzbg"
        }
      }
    }
  }
}